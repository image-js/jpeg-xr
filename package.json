{
  "name": "jpeg-xr",
  "version": "0.0.0",
  "description": "JPEG XR image decoder written entirely in JavaScript",
  "main": "lib/index.js",
  "module": "lib-esm/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "lib-esm",
    "src"
  ],
  "scripts": {
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib lib-esm",
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "npm run tsc",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "test": "npm run test-only && npm run eslint && npm run prettier && npm run check-types",
    "test-only": "jest --coverage",
    "tsc": "npm run clean && npm run tsc-cjs && npm run tsc-esm",
    "tsc-cjs": "tsc --project tsconfig.cjs.json",
    "tsc-esm": "tsc --project tsconfig.esm.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/image-js/jpeg-xr.git"
  },
  "author": "MichaÃ«l Zasso",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/image-js/jpeg-xr/issues"
  },
  "homepage": "https://github.com/image-js/jpeg-xr#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "dependencies": {
    "iobuffer": "^5.1.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "eslint": "^8.7.0",
    "eslint-config-cheminfo-typescript": "^10.3.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.4"
  }
}
